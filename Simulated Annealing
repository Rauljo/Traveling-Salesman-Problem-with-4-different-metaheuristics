    "MAPA = g3\n",
    "\n",
    "def siguiente_solucion(s_act):\n",
    "    \n",
    "    s_aux = s_act[:]\n",
    "    ind1 = random.randint(0, len(s_act)-1)\n",
    "    \n",
    "    elem = s_aux[ind1]\n",
    "    s_aux.pop(ind1)\n",
    "    \n",
    "    while True:\n",
    "        ind2 = random.randint(0, len(s_act)-1)\n",
    "        if (ind2!=ind1):\n",
    "            break\n",
    "            \n",
    "    s_aux.insert(ind2, elem)\n",
    "    \n",
    "    return s_aux"
   ]
